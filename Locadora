print("")
print("Opções de veículos:1- Carro 2- Moto 3- Caminhão")
escolha = int(input("Qual veículo gostaria de alugar?"              ))
while True: 
  match escolha: 
    #CARRO
    case 1:
      print("Opções de carro:")
      print("1- Sedan(R$192.00 a R$215.00 diário) 2- hatch(R$57 a R$80.00 diária) 3- Picape(R$210.00 a R$230.00 diária) 4- Esportivo(R$250.00 a R$300.00 diária)")
      carrotipo = int(input("Qual tipo de carro você que:"))
      match carrotipo:
      #SEDAN     
        case 1:
          print("Ano de fabricação do veículo: ")
          print("1-2015 2- 2020 3- 2023")
          anocarro = int(input("Qual ano do veículo você quer? "))
          match anocarro:
            #2015
            case 1:
              diacomcarro = int(input("Quantos dias você quer ficar com o carro?"))
              valortotal = print ("O valor total será de:", diacomcarro*192, "reais")
              escolhayoun = str(input("Deseja continuar?") )   
              if escolhayoun[0].lower() == 's':
                print("Opções de pagamento:")
                print("1-Á vista 2-parcelado")
                tipopaga = int(input("Qual o método de pagamento?"))
                valortotal = (diacomcarro*192)
                if tipopaga <= 1:            
                 print ("Você terá que devolver o veículo daqui a: ", diacomcarro, "dias")
                 print("A Locadora Henrique agradece a preferencia!")
                else:              
                 print("Opções de parcelamento:")
                 print("1- 2x 2- 4x")
                 parcela = int(input("Quantas vezes você quer parcerlar?"))
                 if parcela == 1:
                    print("O valor mensal a pagar será de :", valortotal/2)
                    print ("Você terá que devolver o veículo daqui a: ", diacomcarro, "dias")
                    print("A Locadora Henrique agradece a preferencia!")
                 elif parcela == 2:
                    print("O valor mensal a pagar será de :", valortotal/4)
                    print ("Você terá que devolver o veículo daqui a: ", diacomcarro, "dias")
                    print("A Locadora Henrique agradece a preferencia!")
                    break
            #2020
            case 2:
              diacomcarro = int(input("Quantos dias você quer ficar com o carro?"))
